<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>

    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
        <link rel="stylesheet" href="{{ asset('/css/style.css') }}">
        <link rel="stylesheet" href="{{ asset('/css/form.css') }}">

    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
</head>
<body>
{% block body %}
    <div class="container-fluid row gx-0">
        <div class="bg-blue col-20">
            {% block left %}
            {% endblock %}
        </div>
        <div class="col-80 gx-1">
            {% block right %}
            {% endblock %}
        </div>
    </div>
{% endblock %}
{% block js %}{% endblock %}

</body>
</html>
